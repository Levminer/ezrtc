{
	"compilerOptions": {
		"esModuleInterop": true,
		"skipLibCheck": true,
		"target": "ESNext",
		"allowJs": true,
		"resolveJsonModule": true,
		"moduleDetection": "force",
		"isolatedModules": true,
		"strict": true,
		"noUncheckedIndexedAccess": true,
		"module": "NodeNext",
		"noEmit": true,
		"declaration": true,
		"declarationMap": true,
		"lib": ["ES2022", "dom", "dom.iterable"]
	}
}
